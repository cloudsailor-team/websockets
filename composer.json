{
  "name" : "ipub/websockets",
  "type" : "library",
  "description" : "Extension for implementing WebSockets server into Nette Framework",
  "keywords" : [
    "nette",
    "websockets",
    "webbsocket-server",
    "ipub",
    "ipublikuj"
  ],

  "homepage" : "https://github.com/iPublikuj/websockets",

  "license" : [
    "GPL-2.0",
    "GPL-3.0"
  ],

  "authors" : [
    {
      "name" : "iPublikuj:cms",
      "email" : "info@ipublikuj.eu",
      "homepage" : "https://www.ipublikuj.eu/"
    }
  ],

  "support" : {
    "email" : "support@ipublikuj.eu",
    "issues" : "https://github.com/iPublikuj/websockets/issues"
  },

  "extra" : {
    "ipub" : {
      "configuration" : {
        "extension" : "IPub\\WebSockets\\DI\\WebSocketsExtension"
      }
    }
  },

  "require" : {
    "php" : ">=7.3.0",

    "ext-simplexml" : "*",
    "ext-mbstring" : "*",
    "ext-iconv" : "*",

    "react/socket" : "~1.2",

    "psr/log" : "~1.0",

    "fig/http-message-util" : "~1.1",

    "nette/di" : "~3.0",
    "nette/http" : "~3.0",
    "nette/utils" : "~3.0",
    "nette/security" : "~3.0"
  },

  "require-dev" : {
    "nette/bootstrap" : "~3.0",
    "nette/mail" : "~3.0",
    "nette/robot-loader" : "~3.0",
    "nette/safe-stream" : "~2.3",
    "nette/tester" : "~2.3",

    "tracy/tracy" : "~2.4",

    "pds/skeleton" : "~1.0"
  },

  "autoload" : {
    "psr-0" : {
      "IPub\\WebSockets\\" : "src/"
    }
  },

  "bin" : [
    "bin/websockets-server",
    "bin/websockets-server.php"
  ]
}
